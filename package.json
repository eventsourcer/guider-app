  "version": "0.0.0",
  "eslintConfig": {
    "extends": [
      "@repo/eslint-config"
    ],
    "ignorePatterns": [
      "out/**/*",
      ".next/**/*"
    ]
  },
  "prettier": "@repo/prettier-config",
